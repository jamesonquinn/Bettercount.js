form(method="post", action="/election/new")
  fieldset
    legend=title
    .clearfix
      label(for="title") Title
      .input
        input.xlarge#title(type="text", name="post[title]")
    .clearfix
      label(for="election") Election
      .input
        textarea#numvoters.xlarge(name="post[numvoters]")
    .actions
      input.btn.primary(type="submit", value="Start!")
      |   
      a.btn(href="/") Cancel